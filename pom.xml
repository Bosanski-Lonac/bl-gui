  <groupId>com.bosanskilonac</groupId>
  <artifactId>bosanski-lonac</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>bosanski-lonac</name>
  <description>Aplikacija projekta bosanski lonac.</description>
  
  <properties>
